{
  "name": "hapi-postgres-boilerplate",
  "version": "0.0.0",
  "description": "Hapijs Server boilerplate",
  "main": "lib/index.js",
  "scripts": {
    "test": "NODE_ENV=test lab -v -c -a code -L",
    "coveralls": "echo repo_token: $COVERALLS_HAPI_POSTGRES_BOILERPLATE > .coveralls.yml",
    "coverage": "NODE_ENV=test npm run coveralls && lab -c -a code -r lcov -o lcov.info && cat ./lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm ./lcov.info .coveralls.yml",
    "start": "nodemon lib/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinicius0026/hapi-postgres-boilerplate.git"
  },
  "keywords": [
    "hapijs",
    "boilerplate"
  ],
  "author": "Vinicius Teixeira <vinicius0026@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vinicius0026/hapi-postgres-boilerplate/issues"
  },
  "homepage": "https://github.com/vinicius0026/hapi-postgres-boilerplate#readme",
  "devDependencies": {
    "code": "^4.0.0",
    "coveralls": "^2.11.15",
    "lab": "^13.0.1",
    "nodemon": "^1.11.0",
    "standard": "^9.0.1"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "boom": "^4.2.0",
    "confidence": "^3.0.2",
    "dotenv": "^4.0.0",
    "glue": "^4.1.0",
    "good": "^7.1.0",
    "good-console": "^6.2.0",
    "good-file": "^6.0.1",
    "good-squeeze": "^5.0.1",
    "hapi": "^16.1.0",
    "hapi-auth-cookie": "^6.1.1",
    "inert": "^4.1.0",
    "joi": "^10.1.0",
    "lout": "^10.0.0",
    "rethinkdb-migrate": "^1.0.2",
    "rethinkdbdash": "^2.3.28",
    "vision": "^4.1.1"
  }
}
